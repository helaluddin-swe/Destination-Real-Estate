generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @map("_id") @default(auto()) @db.ObjectId
  username   String   @unique
  email      String   @unique
  password   String
  avatar     String?
  createdAt  DateTime @default(now())
}
